{
    "name": "cczzjj/di",
    "type": "library",
    "keywords": ["di", "dependency injection", "attributes", "container", "ioc", "psr-11", "psr11"],
    "license": "MIT",
    "description": "Simple Dependencies Container following PSR-11 standards, using php8 Attributes like Spring.",
    "autoload": {
        "psr-4": {
            "DI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "require": {
        "php": ">=8.0",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
